<div class="page-header">
  <h3>Package #<%= @package.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Location</dt>
      <dd><%= @package.location_id %></dd>

      <dt>Resident</dt>
      <dd><%= @package.resident_id %></dd>

      <dt>Contact time</dt>
      <dd><%= @package.contact_time %></dd>

      <dt>Contact type</dt>
      <dd><%= @package.contact_type %></dd>

      <dt>Employee</dt>
      <dd><%= @package.employee_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/packages" class="btn btn-primary">
        Back
      </a>
      <a href="/packages/<%= @package.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_package/<%= @package.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
