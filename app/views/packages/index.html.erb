<div class="page-header">
  <h1>
    Packages
    <a href="/packages/new" class="btn btn-lg btn-success">New Package</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Location</th>
        <th>Resident</th>
        <th>Contact time</th>
        <th>Contact type</th>
        <th>Employee</th>
        <th>Actions</th>
      </tr>

      <% @packages.each do |package| %>
      <tr>
        <td><%= package.location.name %></td>
        <td><%= package.resident.apartment_id %></td>
        <td><%= package.contact_time %></td>
        <td><%= package.contact_type %></td>
        <td><%= package.employee.user_id %></td>
        <td>
          <a href="/packages/<%= package.id %>" class="btn btn-primary">Show</a>
          <a href="/packages/<%= package.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_package/<%= package.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
    <%= paginate @packages, theme: 'twitter-bootstrap-4' %>
  </div>
</div>
